author = {Charles-Smith, Lauren E. and Reynolds, Tera L. and Cameron, Mark A. and Conway, Mike and Lau, Eric H.Y. and Olsen, Jennifer M. and Pavlin, Julie A. and Shigematsu, Mika and Streichert, Laura C. and Suda, Katie J. and Corley, Courtney D.},
booktitle = {PLoS ONE},
doi = {10.1371/journal.pone.0139701},
